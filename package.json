{
  "name": "react-vis-master",
  "version": "1.11.7",
  "license": "MIT",
  "author": "Visualization Team <visualization@uber.com>",
  "description": "Data visualization library based on React and d3.",
  "private": true,
  "workspaces": [
    "website",
    "showcase",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/uber-common/react-vis.git"
  },
  "bugs": {
    "url": "https://github.com/uber-common/react-vis/issues/new",
    "email": "visualization@uber.com"
  },
  "keywords": [
    "d3",
    "react",
    "visualization",
    "chart",
    "es6",
    "babel"
  ],
  "babel": {
    "plugins": [
      [
        "module-resolver",
        {
          "root": [
            "./src"
          ]
        }
      ]
    ],
    "env": {
      "production": {
        "presets": [
          "es2015",
          "stage-0",
          "react"
        ]
      },
      "development": {
        "presets": [
          "es2015",
          "stage-0",
          "react"
        ]
      },
      "es": {
        "presets": [
          [
            "es2015",
            {
              "modules": false
            }
          ],
          "stage-0",
          "react"
        ]
      }
    }
  },
  "nyc": {
    "exclude": [
      "tests/**/*.js",
      "src/utils/react-utils.js"
    ]
  },
  "engines": {
    "node": ">=0.10.0",
    "npm": ">=3.0"
  }
}
